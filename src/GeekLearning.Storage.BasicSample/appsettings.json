{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Verbose",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Storage": {
    "Stores": {
      "Packages": {
        "Provider": "Azure",
        "Parameters": {
          "ConnectionString": "toto",
          "Container": "packages"
        }
      },
      "Assets": {
        "Provider": "Azure",
        "Parameters": {
          "ConnectionString": "toto",
          "Container": "assets"
        }
      },
      "Templates": {
        "Provider": "FileSystem",
        "Parameters": {
          "Path": "Templates"
        }
      }
    }
  }
}
